#
#  standard superscipted numeric format
#
C1
I bibinc.fullnames
H
T AT
.so \*(l]/bmac.std
.\" redefine string for citations
.ds [[ \s-2\u
.ds ]] \d\s+2
.ds [. .
.ds .] " \&
.ds [, ,
.ds ,] " \&
.ds [? ?
.ds ?] " \&
.ds [: :
.ds :] " \&
.ds [; ;
.ds ;] " \&
.ds [! !
.ds !] " \&
.ds [" \&"
.ds "] " \&
.ds [' '
.ds '] " \&
.ds [< " \&
.ds >]
.de s[   \" start reference
.nh
.IP \\*([F. 5n
..
