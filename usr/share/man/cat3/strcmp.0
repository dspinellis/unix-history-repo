     ##iinncclluuddee <<ssttrriinngg..hh>>

     _i_n_t
     ssttrrccmmpp(_c_o_n_s_t _c_h_a_r _*_s_1, _c_o_n_s_t _c_h_a_r _*_s_2);

     _i_n_t
