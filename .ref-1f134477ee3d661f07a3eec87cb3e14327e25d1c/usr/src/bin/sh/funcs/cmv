# Conditional move--don't replace an existing file.

cmv() {
	if test $# != 2
	then	echo "cmv: arg count"
		return 2
	fi
	if test -f "$2" -o -w "$2"
	then	echo "$2 exists"
		return 2
	fi
	/bin/mv "$1" "$2"
}
