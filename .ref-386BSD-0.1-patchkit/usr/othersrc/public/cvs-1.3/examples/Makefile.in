SHELL = /bin/sh

srcdir = @srcdir@
@VPATH@

DISTFILES = Makefile.in commitinfo loginfo modules rcsinfo

all:
.PHONY: all

install: all
.PHONY: install

tags:
.PHONY: tags

TAGS:
.PHONY: TAGS

ls:
	@echo $(DISTFILES)
.PHONY: ls

clean:
	/bin/rm -f *.o core
.PHONY: clean

distclean: clean
	rm -f Makefile
.PHONY: distclean

realclean: distclean
.PHONY: realclean

dist:
	ln $(DISTFILES) ../`cat ../.fname`/examples
.PHONY: dist
